<!DOCTYPE html>
<html>
<head>
  <title>EjemplosRoR</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<p class="notice"><%= notice %></p>                                                                                                                 
<p class="alert"><%= alert %></p> 
  <% if account_signed_in? %>
  <%=link_to "Cerrar session", destroy_account_session_path, method: :delete %>
  <% else %>
  <%=link_to "Iniciar sesion", new_account_session_path %>
  <%=link_to "Crear cuenta", new_account_registration_path %>
  <% end %>
  <%= yield %>
</body>
</html>
